    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-epic-spinners": "0.5.0",
    "react-highlight-words": "0.20.0",
    "react-imask": "^6.6.3",
    "react-markdown": "^6.0.1",
    "react-redux": "^7.2.2",
    "react-router": "6.16.0",
    "react-router-dom": "6.16.0",
    "react-scripts": "5.0.1",
