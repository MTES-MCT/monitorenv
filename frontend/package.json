{
  "name": "monitorenv-frontend",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": "18",
    "npm": "8"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build-storybook": "build-storybook -s public",
    "cypress:open": "cypress open --browser firefox --e2e  --config-file ./cypress.config.ts --e2e",
    "cypress:run": "cypress run --browser firefox --e2e  --config-file ./cypress.config.ts --e2e",
    "dev": "BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "eject": "react-scripts eject",
    "prepare": "cd .. && husky install ./frontend/.husky",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "test:e2e": "npm run cypress:run  --config-file ./cypress.config.ts --e2e",
    "test:lint": "eslint ./src",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test:lint:partial": "eslint --config=./.eslintrc.partial.js ./src",
    "test:lint:partial:fix": "npm run test:lint:partial -- --fix",
    "test:type": "tsc",
    "test:type:partial": "tsc --project ./tsconfig.partial.json",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "^4.0.3",
    "@dnd-kit/modifiers": "^4.0.0",
    "@mtes-mct/monitor-ui": "7.3.1",
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/browser": "7.47.0",
    "@sentry/react": "7.55.2",
    "@sentry/tracing": "7.47.0",
    "@svgr/webpack": "^5.5.0",
    "@tanstack/react-table": "^8.8.5",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "date-fns": "2.29.2",
    "dayjs": "1.11.7",
    "flexsearch": "0.7.21",
    "formik": "2.2.9",
    "hammerjs": "2.0.8",
    "imask": "^6.6.1",
    "lodash": "^4.17.21",
    "ol": "6.14.1",
    "ol-mapbox-style": "6.9.0",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-coordinate-input": "^1.0.0-rc.2",
    "react-device-detect": "2.2.2",
    "react-dom": "18.2.0",
    "react-epic-spinners": "^0.5.0",
    "react-highlight-words": "0.18.0",
    "react-imask": "^6.1.0",
    "react-markdown": "^6.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-toastify": "9.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "2.4.1",
    "rsuite": "5.31.0",
    "styled-components": "5.3.10",
    "timeago.js": "^4.0.2",
    "uuid": "^9.0.0",
    "worker-loader": "^3.0.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@ivangabriele/eslint-config-typescript-react": "4.2.0",
    "@ivangabriele/prettier-config": "3.1.0",
    "@jest/globals": "28.1.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
