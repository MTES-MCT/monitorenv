  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "//": "https://github.com/okonet/lint-staged/issues/825#issuecomment-674575655",
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
