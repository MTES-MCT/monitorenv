{
  "name": "monitorenv-frontend",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "cypress:open": "cypress open --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "cypress:run": "cypress run --browser firefox --config-file ./config/cypress.config.ts --e2e",
    "dev": "dotenv -e ../infra/.env -v BROWSER=none -v DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "eject": "react-scripts eject",
    "prepare": "cd .. && husky install ./frontend/.husky",
    "start": "react-scripts start",
    "test:e2e": "npm run cypress:run --browser firefox --config-file ./cypress.config.ts --e2e",
    "test:multi-windows:open": "jest --config=./config/multi-windows/jest.config.js --detectOpenHandles",
    "test:multi-windows:run": "CI=true jest --config=./config/multi-windows/jest.config.js --detectOpenHandles",
    "test:lint": "eslint ./src",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test:lint:partial": "eslint --config=./.eslintrc.partial.js ./src ./puppeteer",
    "test:lint:partial:fix": "npm run test:lint:partial -- --fix",
    "test:type": "tsc",
    "test:type:partial": "tsc --project ./tsconfig.partial.json",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@mtes-mct/monitor-ui": "11.3.0",
    "@reduxjs/toolkit": "1.9.7",
    "@rsuite/responsive-nav": "5.0.1",
    "@sentry/browser": "7.73.0",
    "@sentry/react": "7.73.0",
    "@sentry/tracing": "7.73.0",
    "@svgr/webpack": "8.0.1",
    "@tanstack/react-table": "8.9.7",
    "@tanstack/react-virtual": "beta",
