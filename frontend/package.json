  "proxy": "http://localhost:8880",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "build:local": "REACT_APP_ENV=local npm run build",
    "lint": "eslint src/ --fix",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@codemirror)/\"",
    "cypress:open": "cypress open --browser firefox --e2e",
    "cypress:run": "cypress run --browser firefox --e2e",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test:lint": "eslint ./src",
    "test:lint:partial": "eslint --config=./.eslintrc.partial.js ./src",
    "test:type": "tsc",
    "test:type:partial": "tsc --project ./tsconfig.partial.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ivangabriele/eslint-config-typescript-react": "4.3.0",
    "@ivangabriele/prettier-config": "3.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@swc/core": "1.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@tsconfig/create-react-app": "1.0.2",
    "@types/jest": "^26.0.20",
    "@types/node": "18.7.17",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "chromatic": "^6.7.0",
    "cypress": "10.7.0",
    "cypress-mouse-position": "^1.0.0",
    "cypress-plugin-snapshots": "github:ivangabriele/cypress-plugin-snapshots",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "webpack": "^5.72.0"
  },
  "overrides": {
    "react-refresh": "0.11.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
